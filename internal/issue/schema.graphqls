# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Issue {
  _id: ID!
  createdAt: Time
  updatedAt: Time
  title: String!
  code: String!
  description: String!
  completed: Boolean!
}

type Query {
  issues: [Issue!]!
}

